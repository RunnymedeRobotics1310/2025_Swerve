{
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.5.2",
    "prettier-plugin-java": "^2.6.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{,java}": [
      "prettier --write",
      "git add"
    ]
  }
}
